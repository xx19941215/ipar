public function uploadFigurePost()
{
    if (!$img_file = $this-&gt;request-&gt;files-&gt;get('img')) {
        return $this-&gt;badRequest();
    }
    $img_tool = image_tool();
    $pack = $img_tool-&gt;save($img_file);
    if (!$pack-&gt;isOk()) {
        return $pack-&gt;toArray();
    }
    $image = $pack-&gt;getItem('image');
    $image-&gt;resize('small', ['w' =&gt; 100]);
    $image-&gt;resize('large', ['w' =&gt; 600]);
    $image-&gt;resize('cover', ['w' =&gt; 235, 'h' =&gt; 175]);
    
    return [
        'ok' =&gt; 1,
        'img' =&gt; [
            'site' =&gt; config()-&gt;get('img.site'),
            'dir' =&gt; $image-&gt;dir,
            'name' =&gt; $image-&gt;name,
            'ext' =&gt; $image-&gt;ext
        ],
    ];  
}
