<div class="tag-dst-row" id="tag-dst-row-{{tag_dst_id}}" data-tag_dst_id="{{tag_dst_id}}">
    <div class="clearfix">
        <div class="tag left">
            <div class="tag-title">{{tag_title}}</div>
        </div>
        <div class="right">
            <a class="tag-vote" href="javascript:;">
                <span class="vote-text">{{vote_text}}</span>
                <span class="vote-count">{{vote_count}}</span>
            </a>
            <a class="tag-op {{op_class}}" href="javascript:;">
                <span class="item vote-wrap">+</span>
                <span class="item unvote-wrap">-</span>
            </a>
        </div>
    </div>
    <div class="vote-user-wrap hide clearfix"></div>
</div>
