<div class="brand-product-row" id="brand-product-row-{{brand_product_id}}" data-brand_product_id="{{brand_product_id}}">
    <div class="clearfix">
        <div class="brand left">
            <div class="brand-title">{{brand_title}}</div>
        </div>
        <div class="right">
            <a class="brand-op {{op_class}}" href="javascript:;">
                <span class="item vote-wrap">+</span>
                <span class="item unvote-wrap">-</span>
            </a>
        </div>
    </div>
    <div class="vote-user-wrap hide clearfix"></div>
</div>
