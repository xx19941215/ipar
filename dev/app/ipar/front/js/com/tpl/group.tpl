<div class="box-wrap">
    <div class="group box">
        <div class="box-cover icon">
            <a class="link" href="{{url}}" target="_blank">
                <div class="group-logo">
                    {{logo}}
                </div>
                <div class="group-back">
                    <span class="group-title">{{fullname}}</span>
                    <span class="group-address">{{address}}</span>
                </div>
            </a>
        </div>

        <div class="box-content">
            <a href="{{url}}">{{content}}</a>
        </div>
        <div class="box-ops">
            <span class="like">
                <a href="#"><i class="icon"></i></a>
                <a href="#"></a>
                <a href="#"></a>
            </span>
            <span class="comment">
                <a href="#"><i class="icon"></i></a>
                <a href="#"></a>
                <a href="#"></a>
            </span>
        </div>

    </div>


</div>