<form class="recommend-form panel-form">
    <input type="hidden" name="src_type_key" value="{{src_type_key}}">
    <input type="hidden" name="src_eid" value="{{src_eid}}">
    <input type="hidden" name="dst_type_key" value="{{dst_type_key}}">
    <input type="hidden" name="dst_eid" value="{{dst_eid}}">

    <div class="title-wrap">
        <div class="title-box"><span class="title">{{title}}</span><i class="icon icon-close close"></i></div>
    </div>

    <div class="content-wrap"><div class="content">{{content}}</div></div>
    <div class="button-wrap">
        <a href="javascript:;" class="cancel">{{cancel}}</a>
        <input type="submit" class="button small" value="{{submit}}">
    </div>
</form>

